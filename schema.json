{
    "name": "aws-miniflux",
    "resources": {
        "aws-miniflux:index:MinifluxService": {
            "isComponent": true,
            "inputProperties": {
                "dbName": {
                    "type": "string",
                    "description": "The name of the PostgreSQL database to be used by Miniflux.",
                    "default": "miniflux"
                },
                "dbUsername": {
                    "type": "string",
                    "description": "The username of the PostgreSQL account to be used by the Miniflux service.",
                    "default": "miniflux"
                },
                "dbPassword": {
                    "type": "string",
                    "description": "The PostgreSQL user's password."
                },
                "adminUsername": {
                    "type": "string",
                    "description": "The username to use for the Miniflux service administrator account.",
                    "default": "admin"
                },
                "adminPassword": {
                    "type": "string",
                    "description": "The Miniflux administrator's password."
                }
            },
            "requiredInputs": [
                "adminPassword",
                "dbPassword"
            ],
            "properties": {
                "endpoint": {
                    "type": "string",
                    "description": "The URL of the Miniflux service."
                }
            },
            "required": [
                "endpoint"
            ]
        }
    },
    "language": {
        "csharp": {
            "packageReferences": {
                "Pulumi": "3.*",
                "Pulumi.Aws": "4.*"
            },
            "namespaces": {
                "aws-miniflux": "AwsMiniflux"
            }
        },
        "go": {
            "generateResourceContainerTypes": true,
            "importBasePath": "github.com/pulumi/pulumi-aws-miniflux/sdk/go/miniflux"
        },
        "nodejs": {
            "dependencies": {
                "@pulumi/aws": "^4.0.0"
            },
            "devDependencies": {
                "typescript": "^3.7.0"
            },
            "packageName": "@pulumi/aws-miniflux"
        },
        "python": {
            "requires": {
                "pulumi": ">=3.0.0,<4.0.0",
                "pulumi-aws": ">=4.0.0,<5.0.0"
            }
        }
    }
}
